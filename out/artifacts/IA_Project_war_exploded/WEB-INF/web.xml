<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd"
         version="3.1">
    <servlet>
        <servlet-name>SignUpCtl</servlet-name>
        <servlet-class>com.controller.SignUpCtl</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>SignUpCtl</servlet-name>
        <url-pattern>/SignUpCtl</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>LogInCtl</servlet-name>
        <servlet-class>com.controller.LogInCtl</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>LogInCtl</servlet-name>
        <url-pattern>/LogInCtl</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>LogOutCtl</servlet-name>
        <servlet-class>com.controller.LogOutCtl</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>LogOutCtl</servlet-name>
        <url-pattern>/LogOutCtl</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>AddFormCtl</servlet-name>
        <servlet-class>com.controller.AddFormCtl</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>AddFormCtl</servlet-name>
        <url-pattern>/AddFormCtl</url-pattern>
    </servlet-mapping>
    
    <servlet>
        <servlet-name>SuspendFormCtl</servlet-name>
        <servlet-class>com.controller.SuspendFormCtl</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>SuspendFormCtl</servlet-name>
        <url-pattern>/SuspendFormCtl</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>CloseFormCtl</servlet-name>
        <servlet-class>com.controller.CloseFormCtl</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>CloseFormCtl</servlet-name>
        <url-pattern>/CloseFormCtl</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>ShowStatCtl</servlet-name>
        <servlet-class>com.controller.ShowStatCtl</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>ShowStatCtl</servlet-name>
        <url-pattern>/ShowStatCtl</url-pattern>
    </servlet-mapping>
</web-app>